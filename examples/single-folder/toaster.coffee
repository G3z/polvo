toast
  
  # src folders
  folders: 'src':''

  # excluded items (default=[])
  exclude: []

  # removes coffee safaty wrapper (default=true)
  bare: true

  # project nature, can be 'browser' or 'desktop'
  nature: 'browser':
    base_url: 'js' # default=''

  # location for the release file
  release: 'www/js/app.js'